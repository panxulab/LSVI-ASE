{
    "env": [
      {
        "name": ["PitfallNoFrameskip-v4"],
        "scale_obs": [0],
        "train_num": [1],
        "test_num": [5]
      }
    ],
    "device": ["cuda"],
    "agent": [{
      "name": ["FGPriorDQN"], 
      "update_num": [1],
      "eta": [0.001, 0.01],
      "is_double": [true]
    }],
    "save_model": [false],
    "show_progress": [false],
    "resume_from_log": [true],
    "save_interval": [5],
    "epoch": [125],
    "n_step": [3],
    "step_per_epoch": [1e5],
    "step_per_collect": [10],
    "update_per_step": [0.1],
    "batch_size": [32],
    "target_update_steps": [500],
    "buffer_size": [1e6],
    "frames_stack": [4],
    "optimizer": [{
        "name": ["aULMC"],
        "kwargs": [{
          "lr": [1e-3,1e-4], 
          "noise_scale": [1e-4,1e-5,1e-6], 
          "a": [0.1,0.01],
          "gamma": [10]
        }]
      }],
    "discount": [0.99],
    "seed": [1],
    "gradient_clip": [-1],
    "generate_random_seed": [true]
  }