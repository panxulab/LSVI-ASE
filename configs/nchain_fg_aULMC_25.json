{
    "env": [{
      "name": ["NChain-v1"],
      "cfg": [{"n": [25]}],
      "train_num": [1],
      "test_num": [20]
    }],
    "device": ["cpu"],
    "agent": [{
      "name": ["FGPriorDQN"], 
      "update_num": [4],
      "eta": [0.01, 0.1, 1],
      "is_double": [false]
    }],
    "save_model": [false],
    "show_progress": [false],
    "resume_from_log": [true],
    "save_interval": [5],
    "epoch": [100],
    "n_step": [1],
    "step_per_epoch": [1e3],
    "step_per_collect": [10],
    "update_per_step": [1.0],
    "batch_size": [32],
    "target_update_steps": [100],  
    "buffer_size": [1e4],
    "frames_stack": [1],
    "optimizer": [
      {
        "name": ["aULMC"],
        "kwargs": [{
          "lr": [1e-2,1e-3],
          "noise_scale": [1e-4,1e-5,1e-6], 
          "a": [1, 0.1,0.01],
          "gamma": [0.01, 0.1, 1]
        }]
      }
    ],
    "discount": [0.99],
    "seed": [1],
    "gradient_clip": [-1],
    "generate_random_seed": [true]
  }