{
    "env": [
      {
        "name": ["SolarisNoFrameskip-v4"],
        "scale_obs": [0],
        "train_num": [1],
        "test_num": [5]
      }
    ],
    "device": ["cuda"],
    "agent": [{
        "name": ["FGPriorDQN"], 
        "update_num": [1],
        "eta": [0.00001, 0.0001, 0.001, 0.01, 0.1, 1],
        "is_double": [true]
      }],
    "save_model": [false],
    "show_progress": [false],
    "resume_from_log": [true],
    "save_interval": [5],
    "epoch": [125],
    "n_step": [3],
    "step_per_epoch": [1e5],
    "step_per_collect": [10],
    "update_per_step": [0.1],
    "batch_size": [32],
    "target_update_steps": [500],
    "buffer_size": [1e6],
    "frames_stack": [4],
    "optimizer": [{
        "name": ["aSGLD"],
        "kwargs": [{
          "lr": [1e-4, 1e-3], "noise_scale": [1e-7],
          "a": [0.01]
        }]
      }],
    "discount": [0.99],
    "seed": [1],
    "gradient_clip": [-1],
    "generate_random_seed": [true]
  }